-- supported configurations --

1)  cam4_aqua_fv 
    --  aquaplanet, slab ocean, finite volume dynamical core
    --  must link external radiative transfer options from ../ExoRT/3dmodels

2)  cam4_aqua_se   
    --  aquaplanet, slab ocean, se cubed sphere dynamical core
    --  must link external radiative transfer options from ../ExoRT/3dmodels

3)  cam4_land_fv  
    --  land planet, finite volume dynamical core dynamical core
    --  must link external radiative transfer options from ../ExoRT/3dmodels

4)  cam4_mixed_fv
    --  continents + slab ocean, finite volume dynamical core
    --  must link external radiative transfer options from ../ExoRT/3dmodels

5)  cam4_aqua_dynocn_fv (NOT WORKING)
    --  aquaplanet, dynamic ocean model, finite volume dynamical core
    --  must link external radiative transfer options from ../ExoRT/3dmodels

6)  circumbinary
    --  continents + slab ocean, finite volume dycore
    --  src.n28archean radiation included in SoureMods



-- add ons --

1)  carma 
     --  hook up for CARMA for cloud and aerosol treatments
     --  currently works with ../ExoRT/3dmodels/src.cam.n28archean.haze

  


